<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="QueryCardView">
        <attr name="caption" format="string"/>
        <attr name="title" format="string"/>
        <attr name="type" format="string"/>
        <attr name="file_url" format="string"/>
        <attr name="query_id" format="integer"/>
    </declare-styleable>

    <declare-styleable name="AudioView">
        <attr name="audio_src" format="string"/>
        <attr name="autoplay" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CustomVideoView">
        <attr name="video_src" format="string"/>
        <attr name="video_autoplay" format="boolean"/>
        <attr name="video_mute" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CustomImageView">
        <attr name="image_src" format="string"/>
    </declare-styleable>
</resources>